@model IEnumerable<Prsctical_8.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Sex</th>
        <th>Age</th>
    </tr>
    @{
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Sex</td>
                <td>@item.Age</td>
            </tr>
        }
    }
</table>

